{
  "name": "westeros",
  "version": "1.0.0",
  "description": "Listagem das casas de Westeros",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maccheri/westeros.git"
  },
  "author": "Felipe Maccheri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maccheri/westeros/issues"
  },
  "homepage": "https://github.com/maccheri/westeros#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "lodash": "^4.17.11",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "reactstrap": "^6.4.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  }
}
